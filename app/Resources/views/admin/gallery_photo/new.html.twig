{% extends 'admin/base.html.twig' %}

{% block page_header %}Dodawanie zdjęcia{% endblock %}

{% block breadcrumb %}
    <li>
        <i class="fa fa-list"></i>  <a href="{{ path('gallery_index') }}">Lista galerii</a>
    </li>
    <li>
        <i class="fa fa-picture-o"></i>  <a href="{{ path('photo_index', {'gallery_id': gallery.id}) }}">Lista zdjęć</a>
    </li>
    <li class="active">
        <i class="fa fa-plus"></i> Dodawanie zdjęcia
